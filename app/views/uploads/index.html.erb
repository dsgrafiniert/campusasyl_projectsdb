<p id="notice"><%= notice %></p>

<h1><%= t '.listing_2' %></h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @uploads.each do |upload| %>
      <tr>
        <td><%= link_to (t '.show_7'), upload %></td>
        <td><%= link_to (t '.edit_12'), edit_upload_path(upload) %></td>
        <td><%= link_to (t '.destroy_8'), upload, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to (t '.new_upload'), new_upload_path %>
